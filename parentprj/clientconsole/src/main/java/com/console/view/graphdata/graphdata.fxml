        <Label fx:id="labelValues" alignment="CENTER" prefHeight="25.0" prefWidth="251.0" text="X: - Y: -" textAlignment="CENTER" AnchorPane.leftAnchor="550.0" AnchorPane.rightAnchor="10.0" AnchorPane.topAnchor="40.0" />
        <AreaChart fx:id="chart" layoutX="62.0" AnchorPane.bottomAnchor="5.0" AnchorPane.leftAnchor="50.0" AnchorPane.rightAnchor="50.0" AnchorPane.topAnchor="70.0">
            <xAxis>
                <DateAxis side="BOTTOM" />
